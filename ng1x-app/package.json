{
  "name": "ng1x-app",
  "version": "1.0.0",
  "description": "Angular 1.x with Webpack",
  "main": "index.js",
  "author": "joel.binnquist@gmail.com",
  "license": "MIT",
  "scripts": {
    "preinstall": "npm link ../common",
    "start": "webpack-dev-server --inline --progress",
    "test": "karma start --auto-watch",
    "dist": "rm -rf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "build": "rm -rf dist && karma start --single-run --no-auto-watch && rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "postinstall": "typings install"
  },
  "dependencies": {
    "angular": "^1.5.6",
    "bootstrap": "^3.3.6",
    "common": "file:../common",
    "core-js": "^2.4.0",
    "font-awesome": "^4.6.3",
    "jquery": "^3.0.0",
    "lodash": "^4.13.1",
    "ramda": "^0.21.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.6"
  },
  "devDependencies": {
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.1",
    "typescript": "^1.8.9",
    "typings": "^1.0.4",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.9.0"
  }
}
